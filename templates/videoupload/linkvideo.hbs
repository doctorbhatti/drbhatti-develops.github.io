<div class="{{className}}" id="link{{type}}{{dataset.rid}}"
     data-screen={{type}}{{dataset.rid}} data-rid="{{dataset.rid }}" data-selected="0" data-status="{{dataset.status}}" draggable="true">
  <div class="{{type}}TitleCont col-sm-9" id ="videoTitleCont{{dataset.rid}}">
    <div id="{{type}}Title{{dataset.rid}}" class="{{type}}Title tooltip2"></div>
    <div class="controls edit"   data-toggle="congtooltip" title="{{getString 'edittitle'}}">
      <a class="editanch" id ="editVideoTitle{{dataset.rid}}"></a>
    </div>
  </div>

  <div id="controlCont{{type}}{{dataset.rid}}" class="controlCont col-sm-3">
    <div class="controls mainpreview " id="mainp{{type}}{{dataset.rid}}" data-screen="{{type}}{{dataset.rid}}"
         data-toggle="congtooltip" title="{{getString 'clicktoplay'}}" data-rid="{{dataset.rid}}">{{getString 'clicktoplay'}}</div>

    {{#if dataset.status}}
    <div class="controls status" data-status="{{dataset.status}}"  data-toggle="congtooltip" title="{{getString 'disable'}}"><a class="statusanch">status"{{dataset.status}}"</a></div>
    {{else}}
      <div class="controls status" data-status="{{dataset.status}}"  data-toggle="congtooltip" title="{{getString 'enable'}}"><a class="statusanch">status"{{dataset.status}}"</a></div>
      {{/if}}
    <div class="controls delete"  data-toggle="congtooltip" title="{{getString 'delete'}}">
      <a class="deleteanch">{{getString 'delete'}}</a>
    </div>
  </div>
</div>

